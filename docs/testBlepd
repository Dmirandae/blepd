## testing blepd   

library(blepd)
data(distribution)
data(tree)

newTree <- tree



evalTree(newTree, distribution)


swapBL(newTree, distribution)


num <- length(newTree$edge.length)

valoresUnif <- abs(rnorm(num))*abs(runif(num,0,1)*runif(num,1,10))
       
        newTree$edge.length <- valoresUnif


evalTree(newTree, distribution)

##plot(newTree)

swapBL(newTree, distribution)

swapBL(newTree, distribution,model="uniform", nTimes=100)

swapBL(newTree, distribution,model="allswap", nTimes=100)

swapBL(newTree, distribution,model="Uniform", nTimes=10000)
