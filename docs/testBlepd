library(blepd)
data(distribution)
data(tree)


evalTree(tree, distribution)

swapBL(tree, distribution)


newTree <- tree

num <- length(newTree$edge.length)

 valoresUnif <- runif(num,0,1)
        
        newTree$edge.length <- valoresUnif 

swapBL(newTree, distribution,model="uniform")




newTree <- tree

num <- length(newTree$edge.length)

 valoresUnif <- rnorm(num)
        
        newTree$edge.length <- valoresUnif 


swapBL(newTree, distribution,model="uniform", nTimes=1000)

swapBL(newTree, distribution,model="allswap", nTimes=1000)

swapBL(newTree, distribution,model="Uniform") ## to check the error





